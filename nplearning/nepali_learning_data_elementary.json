  "grammar": [
    {
      "id": "elementary_gram_001",
      "title": "Continuous Tense",
      "pattern": "Subject + Verb stem + दै/दै + छु/छ",
      "meaning": "Used for ongoing actions",
      "usage": "म पढ्दैछु (I am reading)",
      "examples": [
        {
          "nepali": "म किताब पढ्दैछु।",
          "romanization": "Ma kitab padhdaichhu.",
          "english": "I am reading a book."
        },
        {
          "nepali": "ऊ खाना खाँदैछ।",
          "romanization": "U khana khadaicha.",
          "english": "He/She is eating food."
        }
      ],
      "level": "Elementary",
      "notes": "Shows action in progress"
    },
    {
      "id": "elementary_gram_002",
      "title": "Future Tense",
      "pattern": "Subject + Verb stem + नेछु/नेछौ/नेछ",
      "meaning": "Used for future actions",
      "usage": "म जानेछु (I will go)",
      "examples": [
        {
          "nepali": "म भोलि आउनेछु।",
          "romanization": "Ma bholi aaunechhu.",
          "english": "I will come tomorrow."
        },
        {
          "nepali": "हामी काम गर्नेछौं।",
          "romanization": "Hami kaam garnechhau.",
          "english": "We will work."
        }
      ],
      "level": "Elementary",
      "notes": "Indicates future actions or intentions"
    },
    {
      "id": "elementary_gram_003",
      "title": "Imperative Mood",
      "pattern": "Verb stem + ओस्/नुहोस्/",
      "meaning": "Used for commands and requests",
      "usage": "बस् (sit - informal), बस्नुहोस् (please sit - polite)",
      "examples": [
        {
          "nepali": "यहाँ बस्नुहोस्।",
          "romanization": "Yaha basnuhos.",
          "english": "Please sit here."
        },
        {
          "nepali": "पानी दिनुहोस्।",
          "romanization": "Pani dinuhos.",
          "english": "Please give water."
        }
      ],
      "level": "Elementary",
      "notes": "Politeness level affects the verb form"
    }
  ],
