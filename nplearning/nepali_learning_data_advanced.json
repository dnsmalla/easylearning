  "grammar": [
    {
      "id": "advanced_gram_001",
      "title": "Causative Verbs",
      "pattern": "Verb stem + आउनु",
      "meaning": "Making someone do something",
      "usage": "गराउनु (to make do), खुवाउनु (to feed)",
      "examples": [
        {
          "nepali": "मैले काम गराएँ।",
          "romanization": "Maile kaam garayen.",
          "english": "I made (someone) work."
        }
      ],
      "level": "Advanced",
      "notes": "Indicates causing an action"
    },
    {
      "id": "advanced_gram_002",
      "title": "Reported Speech",
      "pattern": "Subject + भन्नुभयो कि + reported content",
      "meaning": "Reporting what someone said",
      "usage": "उहाँले भन्नुभयो कि...",
      "examples": [
        {
          "nepali": "उहाँले भन्नुभयो कि उहाँ भोलि आउनुहुन्छ।",
          "romanization": "Uhanle bhannubhayo ki uhã bholi aaunuhunchha.",
          "english": "He/She said that he/she will come tomorrow."
        }
      ],
      "level": "Advanced",
      "notes": "कि introduces the reported clause"
    }
  ],
