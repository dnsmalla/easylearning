  "grammar": [
    {
      "id": "gr_beginner_001",
      "title": "हो/हुँ - To be",
      "pattern": "[Subject] + हो/हुँ",
      "meaning": "to be (copula)",
      "usage": "States what something is or identity",
      "examples": [
        {
          "nepali": "म विद्यार्थी हुँ",
          "romanization": "ma vidyarthi hun",
          "english": "I am a student"
        },
        {
          "nepali": "यो किताब हो",
          "romanization": "yo kitab ho",
          "english": "This is a book"
        }
      ],
      "level": "Beginner",
      "notes": "हुँ (hun) is used with म (I), हो (ho) with third person"
    },
    {
      "id": "gr_beginner_002",
      "title": "छ/छु - Is/Am",
      "pattern": "[Subject] + [Location/State] + छ/छु",
      "meaning": "Is / Am (present tense existence)",
      "usage": "Present tense of 'to be' for states and location",
      "examples": [
        {
          "nepali": "म घरमा छु",
          "romanization": "ma gharma chu",
          "english": "I am at home"
        },
        {
          "nepali": "यो राम्रो छ",
          "romanization": "yo ramro chha",
          "english": "This is good"
        }
      ],
      "level": "Beginner",
      "notes": "छु (chu) for I, छ (chha) for third person"
    },
    {
      "id": "gr_beginner_003",
      "title": "को - Possessive",
      "pattern": "[Noun] + को + [Noun]",
      "meaning": "Of / Possessive marker",
      "usage": "Shows possession or relationship",
      "examples": [
        {
          "nepali": "मेरो नाम राम हो",
          "romanization": "mero naam Ram ho",
          "english": "My name is Ram"
        },
        {
          "nepali": "यो मेरो किताब हो",
          "romanization": "yo mero kitab ho",
          "english": "This is my book"
        }
      ],
      "level": "Beginner",
      "notes": "को changes form: मेरो (my), तेरो (your informal), तपाईंको (your formal)"
    },
    {
      "id": "gr_beginner_004",
      "title": "मा - Locative marker",
      "pattern": "[Place] + मा",
      "meaning": "In / At / On",
      "usage": "Indicates location",
      "examples": [
        {
          "nepali": "म विद्यालयमा छु",
          "romanization": "ma vidyalayama chu",
          "english": "I am at school"
        },
        {
          "nepali": "किताब मेजमा छ",
          "romanization": "kitab mejma chha",
          "english": "The book is on the table"
        }
      ],
      "level": "Beginner",
      "notes": "मा is added directly to the noun"
    },
    {
      "id": "gr_beginner_005",
      "title": "लाई - Dative/Accusative",
      "pattern": "[Noun] + लाई",
      "meaning": "To / For (object marker)",
      "usage": "Marks direct/indirect object or recipient",
      "examples": [
        {
          "nepali": "मलाई राम्रो लाग्छ",
          "romanization": "malai ramro lagchha",
          "english": "I like it"
        },
        {
          "nepali": "उसलाई मद्दत गर",
          "romanization": "uslai maddat gara",
          "english": "Help him/her"
        }
      ],
      "level": "Beginner",
      "notes": "Often used with verbs of liking, giving, telling"
    }
  ],
