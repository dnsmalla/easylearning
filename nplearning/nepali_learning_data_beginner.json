  "grammar": [
    {
      "id": "beginner_gram_001",
      "title": "Simple Present Tense",
      "pattern": "Subject + Verb stem + छु/छौ/छ/छौं/छन्",
      "meaning": "Used for habitual actions and general truths",
      "usage": "म खान्छु (I eat), तिमी खान्छौ (You eat)",
      "examples": [
        {
          "nepali": "म नेपाली बोल्छु।",
          "romanization": "Ma nepali bolchhu.",
          "english": "I speak Nepali."
        },
        {
          "nepali": "ऊ विद्यालय जान्छ।",
          "romanization": "U vidyalaya janchha.",
          "english": "He/She goes to school."
        },
        {
          "nepali": "हामी भात खान्छौं।",
          "romanization": "Hami bhaat khanchhaun.",
          "english": "We eat rice."
        }
      ],
      "level": "Beginner",
      "notes": "The verb ending changes based on the subject"
    },
    {
      "id": "beginner_gram_002",
      "title": "Simple Past Tense",
      "pattern": "Subject + Verb stem + एँ/यौ/यो/यौं/ए",
      "meaning": "Used for completed actions in the past",
      "usage": "म गएँ (I went), ऊ गयो (He went)",
      "examples": [
        {
          "nepali": "म हिजो घर गएँ।",
          "romanization": "Ma hijo ghar gaen.",
          "english": "I went home yesterday."
        },
        {
          "nepali": "तिमीले खाना खायौ।",
          "romanization": "Timile khana khayau.",
          "english": "You ate food."
        },
        {
          "nepali": "उनीहरू आए।",
          "romanization": "Uniharu aaye.",
          "english": "They came."
        }
      ],
      "level": "Beginner",
      "notes": "Past tense formation varies by verb type"
    },
    {
      "id": "beginner_gram_003",
      "title": "Question Formation",
      "pattern": "Question word + Subject + Verb + ?",
      "meaning": "How to form questions in Nepali",
      "usage": "के, को, कहाँ, कसरी, किन, कति",
      "examples": [
        {
          "nepali": "तपाईंको नाम के हो?",
          "romanization": "Tapaiko naam ke ho?",
          "english": "What is your name?"
        },
        {
          "nepali": "तपाईं कहाँ जानुहुन्छ?",
          "romanization": "Tapai kaha januhunchha?",
          "english": "Where are you going?"
        },
        {
          "nepali": "यो कति हो?",
          "romanization": "Yo kati ho?",
          "english": "How much is this?"
        }
      ],
      "level": "Beginner",
      "notes": "Question words come at the beginning of sentences"
    },
    {
      "id": "beginner_gram_004",
      "title": "Negation",
      "pattern": "Subject + Verb stem + दिन/दैन/दिनौं",
      "meaning": "How to form negative sentences",
      "usage": "म जान्दिनँ (I don't go)",
      "examples": [
        {
          "nepali": "म मासु खान्दिनँ।",
          "romanization": "Ma maasu khandina.",
          "english": "I don't eat meat."
        },
        {
          "nepali": "ऊ आउँदैन।",
          "romanization": "U aundaina.",
          "english": "He/She doesn't come."
        },
        {
          "nepali": "हामी थाहा पाउँदैनौं।",
          "romanization": "Hami thaha paundainau.",
          "english": "We don't know."
        }
      ],
      "level": "Beginner",
      "notes": "Negation changes the verb ending"
    },
    {
      "id": "beginner_gram_005",
      "title": "Postpositions",
      "pattern": "Noun + Postposition",
      "meaning": "Nepali uses postpositions instead of prepositions",
      "usage": "मा (in), मा (to), बाट (from), सँग (with)",
      "examples": [
        {
          "nepali": "म घरमा छु।",
          "romanization": "Ma gharma chhu.",
          "english": "I am at home."
        },
        {
          "nepali": "म बजारबाट आएँ।",
          "romanization": "Ma bazarbata aayen.",
          "english": "I came from the market."
        },
        {
          "nepali": "म साथीसँग जान्छु।",
          "romanization": "Ma sathisanga janchhu.",
          "english": "I go with my friend."
        }
      ],
      "level": "Beginner",
      "notes": "Postpositions come after the noun"
    }
  ],
  "practice": [
    {
      "id": "beginner_vocab_q_001",
      "question": "What does 'नमस्ते' mean?",
      "options": [
        "Sorry/Excuse me",
        "Thank you",
        "Hello/Greetings",
        "Please"
      ],
      "correctAnswer": "Hello/Greetings",
      "explanation": "नमस्ते (namaste) means Hello/Greetings. Traditional greeting",
      "category": "vocabulary",
      "level": "Beginner"
    },
    {
      "id": "beginner_vocab_q_002",
      "question": "What does 'धन्यवाद' mean?",
      "options": [
        "Please",
        "Hello/Greetings",
        "Thank you",
        "Sorry/Excuse me"
      ],
      "correctAnswer": "Thank you",
      "explanation": "धन्यवाद (dhanyabad) means Thank you. Expression of gratitude",
      "category": "vocabulary",
      "level": "Beginner"
    },
    {
      "id": "beginner_vocab_q_003",
