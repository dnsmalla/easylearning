  "grammar": [
    {
      "id": "beginner_gram_001",
      "title": "Sanskrit Sentence Structure",
      "pattern": "Subject + Object + Verb",
      "meaning": "Basic word order in Sanskrit",
      "usage": "Sanskrit typically follows SOV (Subject-Object-Verb) order",
      "examples": [
        {
          "sanskrit": "बालकः पुस्तकं पठति।",
          "romanization": "bālakaḥ pustakaṃ paṭhati",
          "english": "The boy reads a book."
        },
        {
          "sanskrit": "बालिका फलं खादति।",
          "romanization": "bālikā phalaṃ khādati",
          "english": "The girl eats a fruit."
        }
      ],
      "level": "Beginner",
      "notes": "The verb typically comes at the end of the sentence"
    },
    {
      "id": "beginner_gram_002",
      "title": "Present Tense - लट् लकार (Laṭ Lakāra)",
      "pattern": "Root + Present Suffix",
      "meaning": "Used for present actions",
      "usage": "पठ् → पठति (paṭh → paṭhati)",
      "examples": [
        {
          "sanskrit": "अहं पठामि।",
          "romanization": "ahaṃ paṭhāmi",
          "english": "I read."
        },
        {
          "sanskrit": "त्वं पठसि।",
          "romanization": "tvaṃ paṭhasi",
          "english": "You read."
        },
        {
          "sanskrit": "सः पठति।",
          "romanization": "saḥ paṭhati",
          "english": "He reads."
        }
      ],
      "level": "Beginner",
      "notes": "Present tense is one of the most commonly used tenses"
    },
    {
      "id": "beginner_gram_003",
      "title": "Gender in Sanskrit",
      "pattern": "Masculine (पुं), Feminine (स्त्री), Neuter (नपुंसक)",
      "meaning": "Every noun has a gender",
      "usage": "बालकः (masculine), बालिका (feminine), पुस्तकम् (neuter)",
      "examples": [
        {
          "sanskrit": "सुन्दरः बालकः।",
          "romanization": "sundaraḥ bālakaḥ",
          "english": "Beautiful boy (masculine)."
        },
        {
          "sanskrit": "सुन्दरा बालिका।",
          "romanization": "sundarā bālikā",
          "english": "Beautiful girl (feminine)."
        },
        {
          "sanskrit": "सुन्दरं पुस्तकम्।",
          "romanization": "sundaraṃ pustakam",
          "english": "Beautiful book (neuter)."
        }
