        "dialogues": [
          {
            "speaker": "Doctor",
            "nepali": "के भयो?",
            "english": "What happened?"
          },
          {
            "speaker": "Patient",
            "nepali": "मलाई दुई दिनदेखि ज्वरो आएको छ।",
