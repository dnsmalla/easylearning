  "grammar": [
    {
      "id": "beginner_gram_001",
      "title": "Simple Present Tense",
      "pattern": "Subject + Verb stem + छु/छौ/छ/छौं/छन्",
      "meaning": "Used for habitual actions and general truths",
      "usage": "म खान्छु (I eat), तिमी खान्छौ (You eat), ऊ खान्छ (He/She eats)",
      "examples": [
        {
          "nepali": "म नेपाली बोल्छु।",
          "romanization": "Ma nepali bolchhu.",
          "english": "I speak Nepali."
        },
        {
          "nepali": "ऊ विद्यालय जान्छ।",
          "romanization": "U vidyalaya janchha.",
          "english": "He/She goes to school."
        },
        {
          "nepali": "हामी दाल भात खान्छौं।",
          "romanization": "Hami daal bhaat khanchaun.",
          "english": "We eat dal bhat."
        }
      ],
      "level": "Beginner",
      "notes": "Present tense endings: छु (I), छौ (you informal), छ (he/she), छौं (we), छन् (they/you formal)"
    },
    {
      "id": "beginner_gram_002",
      "title": "Simple Past Tense",
      "pattern": "Subject + Verb stem + एँ/यौ/यो/यौं/ए",
      "meaning": "Used for completed actions in the past",
      "usage": "म गएँ (I went), ऊ गयो (He went), तपाईं गयौ (You went)",
      "examples": [
        {
          "nepali": "म हिजो घर गएँ।",
          "romanization": "Ma hijo ghar gaen.",
          "english": "I went home yesterday."
        },
        {
          "nepali": "ऊ खाना खायो।",
          "romanization": "U khana khayo.",
          "english": "He/She ate food."
        }
      ],
      "level": "Beginner",
      "notes": "Past tense formation varies by verb type"
    },
    {
      "id": "beginner_gram_003",
      "title": "Question Formation with के, को, कहाँ",
      "pattern": "Question word + Subject/Object + Verb + ?",
      "meaning": "How to form questions in Nepali",
      "usage": "के (what), को (who), कहाँ (where), कसरी (how), किन (why), कति (how much/many)",
      "examples": [
        {
          "nepali": "तपाईंको नाम के हो?",
          "romanization": "Tapaiko naam ke ho?",
          "english": "What is your name?"
        },
        {
          "nepali": "तपाईं कहाँ जानुहुन्छ?",
          "romanization": "Tapai kaha januhunchha?",
          "english": "Where are you going?"
        },
        {
          "nepali": "यो को हो?",
          "romanization": "Yo ko ho?",
          "english": "Who is this?"
        }
      ],
      "level": "Beginner",
      "notes": "Question words typically come before the subject"
    },
    {
      "id": "beginner_gram_004",
      "title": "Negation with दिन/दैन",
      "pattern": "Subject + Verb stem + दिनँ/दैनौ/दैन/दैनौं/दैनन्",
      "meaning": "How to form negative sentences",
      "usage": "म जान्दिनँ (I don't go), ऊ खान्दैन (He doesn't eat)",
      "examples": [
        {
          "nepali": "म मासु खान्दिनँ।",
