  "grammar": [
    {
      "id": "gr_001",
      "pattern": "हो (ho)",
      "title": "हो - To be",
      "meaning": "To be / is",
      "usage": "Used to state facts or identity",
      "examples": [
        {
          "nepali": "म विद्यार्थी हुँ",
          "romanization": "ma vidyarthi hun",
          "english": "I am a student"
        },
        {
          "nepali": "यो किताब हो",
          "romanization": "yo kitab ho",
          "english": "This is a book"
        }
      ],
      "level": "Beginner"
    },
    {
      "id": "gr_002",
      "pattern": "को (ko)",
      "title": "को - Possessive",
      "meaning": "Of / Possessive marker",
      "usage": "Shows possession or relationship",
      "examples": [
        {
          "nepali": "मेरो नाम राज हो",
          "romanization": "mero naam Raj ho",
          "english": "My name is Raj"
        },
        {
          "nepali": "यो मेरो किताब हो",
          "romanization": "yo mero kitab ho",
          "english": "This is my book"
        }
      ],
      "level": "Beginner"
    },
    {
      "id": "gr_003",
      "pattern": "छ (cha)",
      "title": "छ - Is/Are/Am",
      "meaning": "Is / Are / Am (present tense)",
      "usage": "Present tense of 'to be' for states and conditions",
      "examples": [
        {
          "nepali": "म घरमा छु",
          "romanization": "ma gharma chu",
          "english": "I am at home"
        },
        {
          "nepali": "यो राम्रो छ",
          "romanization": "yo ramro cha",
          "english": "This is good"
        }
      ],
      "level": "Beginner"
    },
    {
      "id": "gr_004",
      "pattern": "मा (ma)",
      "title": "मा - Locative marker",
      "meaning": "In / At / On",
          "usage": "Indicates location",
      "examples": [
        {
          "nepali": "म विद्यालयमा छु",
          "romanization": "ma vidyalayama chu",
          "english": "I am at school"
        },
        {
          "nepali": "किताब मेजमा छ",
          "romanization": "kitab mejma cha",
          "english": "The book is on the table"
        }
      ],
      "level": "Beginner"
    },
    {
      "id": "gr_005",
      "pattern": "लाई (lai)",
      "title": "लाई - Dative/Accusative marker",
      "meaning": "To / For (direct/indirect object marker)",
      "usage": "Marks the object of a sentence or recipient of an action",
      "examples": [
        {
          "nepali": "मलाई राम्रो लाग्छ",
          "romanization": "malai ramro lagcha",
          "english": "I like it (lit: to me it feels good)"
        },
        {
          "nepali": "उसलाई मद्दत गर",
          "romanization": "uslai maddat gara",
          "english": "Help him/her"
        }
      ],
      "level": "Elementary"
    }
  ],
