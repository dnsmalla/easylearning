    "gates": {
      "title": "Common Quantum Gates",
      "groups": [
        {
          "id": "single_qubit",
          "title": "Single-Qubit Gates",
          "color": "#6366F1",
          "items": [
            {"name": "I (Identity)", "matrix": "[[1,0],[0,1]]", "action": "No change"},
            {"name": "X (NOT)", "matrix": "[[0,1],[1,0]]", "action": "|0⟩↔|1⟩"},
            {"name": "Y", "matrix": "[[0,-i],[i,0]]", "action": "Bit+phase flip"},
            {"name": "Z", "matrix": "[[1,0],[0,-1]]", "action": "|1⟩→-|1⟩"},
            {"name": "H", "matrix": "(1/√2)[[1,1],[1,-1]]", "action": "|0⟩→|+⟩, |1⟩→|-⟩"},
            {"name": "S (√Z)", "matrix": "[[1,0],[0,i]]", "action": "π/2 phase"},
            {"name": "S† (S-dag)", "matrix": "[[1,0],[0,-i]]", "action": "-π/2 phase"},
            {"name": "T (⁴√Z)", "matrix": "[[1,0],[0,e^(iπ/4)]]", "action": "π/4 phase"},
            {"name": "T† (T-dag)", "matrix": "[[1,0],[0,e^(-iπ/4)]]", "action": "-π/4 phase"},
            {"name": "Rx(θ)", "matrix": "cos(θ/2)I - i·sin(θ/2)X", "action": "Rotate around X"},
            {"name": "Ry(θ)", "matrix": "cos(θ/2)I - i·sin(θ/2)Y", "action": "Rotate around Y"},
            {"name": "Rz(θ)", "matrix": "[[e^(-iθ/2),0],[0,e^(iθ/2)]]", "action": "Rotate around Z"}
          ]
        },
        {
          "id": "two_qubit",
          "title": "Two-Qubit Gates",
          "color": "#EC4899",
          "items": [
            {"name": "CNOT/CX", "action": "Flip target if control=|1⟩"},
            {"name": "CY", "action": "Apply Y to target if control=|1⟩"},
            {"name": "CZ", "action": "Phase flip if both |1⟩"},
            {"name": "SWAP", "action": "Exchange two qubits"},
            {"name": "iSWAP", "action": "SWAP with i phase"},
            {"name": "√SWAP", "action": "Half SWAP"},
            {"name": "CR(θ)", "action": "Controlled phase rotation"},
            {"name": "ECR", "action": "Echoed cross-resonance (IBM native)"}
          ]
        },
        {
          "id": "three_qubit",
          "title": "Three-Qubit Gates",
          "color": "#F59E0B",
          "items": [
            {"name": "Toffoli/CCX", "action": "Flip target if both controls=|1⟩"},
            {"name": "Fredkin/CSWAP", "action": "SWAP if control=|1⟩"},
            {"name": "CCZ", "action": "Phase flip if all three |1⟩"}
          ]
        }
      ]
