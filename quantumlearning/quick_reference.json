    "qiskit_snippets": {
      "title": "Qiskit Code Snippets",
      "groups": [
        {
          "id": "basics",
          "title": "Basic Operations",
          "color": "#10B981",
          "snippets": [
            {
              "name": "Create Circuit",
              "code": "from qiskit import QuantumCircuit\nqc = QuantumCircuit(2, 2)"
            },
            {
              "name": "Bell State",
              "code": "qc.h(0)\nqc.cx(0, 1)"
            },
            {
              "name": "Measure All",
              "code": "qc.measure_all()"
            },
            {
              "name": "Simulate",
              "code": "from qiskit_aer import AerSimulator\nsim = AerSimulator()\nresult = sim.run(qc, shots=1000).result()\ncounts = result.get_counts()"
            }
          ]
        },
        {
          "id": "advanced",
          "title": "Advanced Operations",
          "color": "#8B5CF6",
          "snippets": [
            {
              "name": "Statevector",
              "code": "from qiskit.quantum_info import Statevector\nsv = Statevector.from_instruction(qc)\nprint(sv)"
            },
            {
              "name": "Transpile for Backend",
              "code": "from qiskit import transpile\nqc_t = transpile(qc, backend, optimization_level=3)"
            },
            {
              "name": "Custom Gate",
              "code": "from qiskit.circuit.library import UnitaryGate\nU = np.array([[1,0],[0,-1]])\ngate = UnitaryGate(U)\nqc.append(gate, [0])"
            },
            {
              "name": "Parameterized Circuit",
              "code": "from qiskit.circuit import Parameter\ntheta = Parameter('θ')\nqc.ry(theta, 0)"
            }
          ]
        },
        {
          "id": "ibm_hardware",
          "title": "IBM Hardware",
          "color": "#EC4899",
          "snippets": [
            {
              "name": "Connect to IBM",
              "code": "from qiskit_ibm_runtime import QiskitRuntimeService\nservice = QiskitRuntimeService()"
            },
            {
              "name": "List Backends",
              "code": "for backend in service.backends():\n    print(backend.name)"
            },
            {
              "name": "Run on Hardware",
              "code": "from qiskit_ibm_runtime import Sampler\nsampler = Sampler(session=backend)\njob = sampler.run(qc)\nresult = job.result()"
            }
          ]
        }
      ]
