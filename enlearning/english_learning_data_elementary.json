  "grammar": [
    {
      "id": "elem_gram_001",
      "title": "Past Simple - Regular Verbs",
      "pattern": "Subject + verb + ed",
      "meaning": "Used to talk about completed actions in the past",
      "usage": "Add -ed to regular verbs (worked, played, watched)",
      "examples": [
        {
          "english": "I walked to school yesterday.",
          "translation": "A completed action in the past.",
          "explanation": "walk + ed = walked"
        },
        {
          "english": "She watched a movie last night.",
          "translation": "A completed action in the past.",
          "explanation": "watch + ed = watched"
        }
      ],
      "level": "Elementary",
      "notes": "For verbs ending in -e, just add -d (liked, loved). For verbs ending in consonant + y, change y to i and add -ed (studied, tried)."
    },
    {
      "id": "elem_gram_002",
      "title": "Past Simple - Irregular Verbs",
      "pattern": "Subject + irregular past form",
      "meaning": "Common verbs with irregular past forms",
      "usage": "These must be memorized: go→went, see→saw, eat→ate",
      "examples": [
        {
          "english": "I went to the park.",
          "translation": "A completed action in the past.",
          "explanation": "go → went (irregular)"
        },
        {
          "english": "We ate pizza for dinner.",
          "translation": "A completed action in the past.",
          "explanation": "eat → ate (irregular)"
        }
      ],
      "level": "Elementary",
      "notes": "Common irregular verbs: be→was/were, have→had, do→did, say→said, get→got, make→made, know→knew, think→thought, take→took, come→came"
    },
    {
      "id": "elem_gram_003",
      "title": "Present Continuous",
      "pattern": "Subject + am/is/are + verb-ing",
      "meaning": "Actions happening now or around now",
      "usage": "Use for current actions or temporary situations",
      "examples": [
        {
          "english": "I am reading a book.",
          "translation": "An action happening now.",
          "explanation": "am + read + ing"
        },
        {
          "english": "They are playing soccer.",
          "translation": "An action happening now.",
          "explanation": "are + play + ing"
        }
      ],
      "level": "Elementary",
      "notes": "For verbs ending in -e, drop the e and add -ing (make→making). For short verbs with consonant-vowel-consonant, double the final consonant (run→running)."
    },
    {
      "id": "elem_gram_004",
      "title": "Comparatives",
      "pattern": "adjective + er / more + adjective",
      "meaning": "Comparing two things",
      "usage": "Short adjectives: add -er. Long adjectives: use 'more'",
      "examples": [
        {
          "english": "This book is bigger than that one.",
          "translation": "Comparing sizes.",
          "explanation": "big + er = bigger"
        },
        {
          "english": "English is more interesting than math.",
          "translation": "Comparing subjects.",
          "explanation": "'more' for longer adjectives"
        }
      ],
      "level": "Elementary",
      "notes": "Irregular comparatives: good→better, bad→worse, far→farther/further"
    },
    {
      "id": "elem_gram_005",
      "title": "Can/Can't for Ability",
      "pattern": "Subject + can/can't + base verb",
      "meaning": "Express ability or inability",
      "usage": "Use can for ability, can't for inability",
      "examples": [
        {
          "english": "I can swim.",
          "translation": "I have the ability to swim.",
          "explanation": "Expressing ability"
        },
        {
          "english": "She can't speak Japanese.",
          "translation": "She doesn't have the ability.",
          "explanation": "Expressing inability"
        }
      ],
      "level": "Elementary",
      "notes": "Can is followed by the base form of the verb (no 'to'). Can't is the negative form."
    }
  ],
