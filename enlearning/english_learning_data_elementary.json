  "grammar": [
    {
      "id": "elem_gram_001",
      "title": "Past Simple - Regular Verbs",
      "pattern": "Subject + verb-ed",
      "meaning": "Used to describe completed actions in the past",
      "usage": "Add -ed to regular verbs (work → worked, play → played)",
      "examples": [
        {
          "english": "I worked yesterday.",
          "translation": "An action completed in the past.",
          "explanation": "Regular verb: work + ed = worked"
        },
        {
          "english": "She played tennis last week.",
          "translation": "A past activity.",
          "explanation": "Regular verb: play + ed = played"
        }
      ],
      "level": "Elementary",
      "notes": "Spelling changes: stop → stopped, study → studied"
    },
    {
      "id": "elem_gram_002",
      "title": "Past Simple - Irregular Verbs",
      "pattern": "Subject + past form",
      "meaning": "Common verbs with irregular past forms",
      "usage": "Memorize irregular forms (go → went, have → had, see → saw)",
      "examples": [
        {
          "english": "I went to the store.",
          "translation": "Past action using irregular verb.",
          "explanation": "go → went (irregular)"
        },
        {
          "english": "We had dinner together.",
          "translation": "Past action.",
          "explanation": "have → had (irregular)"
        }
      ],
      "level": "Elementary",
      "notes": "Common irregular verbs: be/was/were, do/did, make/made, say/said"
    }
  ],
