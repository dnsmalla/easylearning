  "grammar": [
    {
      "id": "beginner_gram_001",
      "title": "Present Simple - To Be",
      "pattern": "Subject + am/is/are + ...",
      "meaning": "Used to describe states and facts",
      "usage": "I am, You are, He/She/It is, We are, They are",
      "examples": [
        {
          "english": "I am a student.",
          "translation": "I study at a school or university.",
          "explanation": "Use 'am' with 'I'"
        },
        {
          "english": "She is happy.",
          "translation": "She feels happiness.",
          "explanation": "Use 'is' with he/she/it"
        },
        {
          "english": "They are teachers.",
          "translation": "They work as teachers.",
          "explanation": "Use 'are' with they/we/you"
        }
      ],
      "level": "Beginner",
      "notes": "This is the most basic and important verb in English."
    },
    {
      "id": "beginner_gram_002",
      "title": "Articles - A/An",
      "pattern": "a/an + singular noun",
      "meaning": "Indefinite articles for singular nouns",
      "usage": "Use 'a' before consonant sounds, 'an' before vowel sounds",
      "examples": [
        {
          "english": "I have a book.",
          "translation": "One book, not specific.",
          "explanation": "'a' before consonant sound 'b'"
        },
        {
          "english": "She is an engineer.",
          "translation": "One engineer, not specific.",
          "explanation": "'an' before vowel sound 'e'"
        }
      ],
      "level": "Beginner",
      "notes": "Articles are essential in English."
    },
    {
      "id": "beginner_gram_003",
      "title": "Present Simple - Regular Verbs",
      "pattern": "Subject + verb(+s/es)",
      "meaning": "Used for habits and routines",
      "usage": "Add -s or -es for third person singular (he/she/it)",
      "examples": [
        {
          "english": "I work every day.",
          "translation": "A regular habit or routine.",
          "explanation": "No change for I/you/we/they"
        },
        {
          "english": "He works in a bank.",
          "translation": "His routine job.",
          "explanation": "Add -s for he/she/it"
        }
      ],
      "level": "Beginner",
      "notes": "Most verbs add -s, but some add -es (goes, does, watches)."
    },
    {
      "id": "beginner_gram_004",
      "title": "Plural Nouns",
      "pattern": "noun + s/es",
      "meaning": "Used to show more than one",
      "usage": "Add -s for most nouns, -es for words ending in s, x, ch, sh",
      "examples": [
        {
          "english": "I have two cats.",
          "translation": "More than one cat.",
          "explanation": "Add -s for regular plurals"
        },
        {
          "english": "She has three boxes.",
          "translation": "More than one box.",
          "explanation": "Add -es for words ending in x"
        }
      ],
      "level": "Beginner",
      "notes": "Some nouns have irregular plurals (child/children, man/men)."
    },
    {
      "id": "beginner_gram_005",
      "title": "Possessive Adjectives",
      "pattern": "my/your/his/her/its/our/their + noun",
      "meaning": "Shows who something belongs to",
      "usage": "Use before nouns to show possession",
      "examples": [
        {
          "english": "This is my book.",
          "translation": "The book belongs to me.",
          "explanation": "'my' for I"
        },
        {
          "english": "Her name is Sarah.",
          "translation": "The name belongs to her.",
          "explanation": "'her' for she"
        }
      ],
      "level": "Beginner",
      "notes": "Don't confuse with possessive pronouns (mine, yours, etc.)."
    }
  ],
