{
  "version": "1.0.0",
  "description": "Quick reference data for electrical engineering - formulas, units, prefixes, constants, and color codes",
  "sections": {
    "formulas": {
      "title": "Essential Formulas",
      "groups": [
        {
          "id": "ohms-law",
          "title": "Ohm's Law",
          "color": "#FFD93D",
          "formulas": [
            {"formula": "V = I × R", "description": "Voltage"},
            {"formula": "I = V ÷ R", "description": "Current"},
            {"formula": "R = V ÷ I", "description": "Resistance"}
          ]
        },
        {
          "id": "power",
          "title": "Power Formulas",
          "color": "#FF6B35",
          "formulas": [
            {"formula": "P = V × I", "description": "Power (basic)"},
            {"formula": "P = I² × R", "description": "Power from current"},
            {"formula": "P = V² ÷ R", "description": "Power from voltage"},
            {"formula": "E = P × t", "description": "Energy"}
          ]
        },
        {
          "id": "series",
          "title": "Series Circuits",
          "color": "#4D96FF",
          "formulas": [
            {"formula": "Rₜ = R₁ + R₂ + R₃...", "description": "Total resistance"},
            {"formula": "I = same everywhere", "description": "Current"},
            {"formula": "Vₜ = V₁ + V₂ + V₃...", "description": "Total voltage"}
          ]
        },
        {
          "id": "parallel",
          "title": "Parallel Circuits",
          "color": "#7B2CBF",
          "formulas": [
            {"formula": "1/Rₜ = 1/R₁ + 1/R₂...", "description": "Total resistance"},
            {"formula": "Rₜ = (R₁×R₂)/(R₁+R₂)", "description": "Two resistors"},
            {"formula": "V = same everywhere", "description": "Voltage"},
            {"formula": "Iₜ = I₁ + I₂ + I₃...", "description": "Total current"}
          ]
        },
        {
          "id": "capacitors-inductors",
          "title": "Capacitors & Inductors",
          "color": "#1ABC9C",
          "formulas": [
            {"formula": "τ = R × C", "description": "RC time constant"},
            {"formula": "τ = L ÷ R", "description": "RL time constant"},
            {"formula": "Xc = 1/(2πfC)", "description": "Capacitive reactance"},
            {"formula": "XL = 2πfL", "description": "Inductive reactance"},
            {"formula": "f₀ = 1/(2π√LC)", "description": "Resonant frequency"}
          ]
        },
        {
          "id": "ac-power",
          "title": "AC Power",
          "color": "#E74C3C",
          "formulas": [
            {"formula": "P = VIcos(θ)", "description": "Real power (W)"},
            {"formula": "Q = VIsin(θ)", "description": "Reactive power (VAR)"},
            {"formula": "S = VI", "description": "Apparent power (VA)"},
            {"formula": "PF = cos(θ) = P/S", "description": "Power factor"}
          ]
        },
        {
          "id": "transformers",
          "title": "Transformers",
          "color": "#9B59B6",
          "formulas": [
            {"formula": "V₁/V₂ = N₁/N₂", "description": "Voltage ratio"},
            {"formula": "I₁/I₂ = N₂/N₁", "description": "Current ratio"},
            {"formula": "V₁I₁ = V₂I₂", "description": "Power conservation"}
          ]
        }
      ]
    },
