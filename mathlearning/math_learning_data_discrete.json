        {
          "id": "logic_contrapositive",
          "name": "Contrapositive",
          "formula": "P ‚Üí Q ‚â° ¬¨Q ‚Üí ¬¨P",
          "latex": "P \\rightarrow Q \\equiv \\neg Q \\rightarrow \\neg P",
          "meaning": "Logically equivalent form of implication",
          "category": "laws"
        }
