    {
      "id": "probability_basics",
      "title": "Probability Basics",
      "symbol": "P(A)",
      "level": "middle",
      "definition": {
        "text": "Probability measures the likelihood of an event occurring, expressed as a number between 0 (impossible) and 1 (certain). The probability of an event A is the ratio of favorable outcomes to total possible outcomes in a sample space.",
        "keyTerms": ["sample space", "event", "outcome", "favorable", "complement", "mutually exclusive"]
      },
      "keyFormulas": [
        {
          "id": "prob_basic",
          "name": "Basic Probability",
          "formula": "P(A) = favorable outcomes / total outcomes",
          "latex": "P(A) = \\frac{\\text{favorable outcomes}}{\\text{total outcomes}}",
          "meaning": "Probability of event A occurring",
          "category": "definition"
        },
        {
          "id": "prob_complement",
          "name": "Complement Rule",
          "formula": "P(A') = 1 - P(A)",
          "latex": "P(A') = 1 - P(A)",
          "meaning": "Probability of A NOT occurring",
          "category": "rules"
        },
        {
          "id": "prob_or",
          "name": "Addition Rule (OR)",
          "formula": "P(A ‚à™ B) = P(A) + P(B) - P(A ‚à© B)",
          "latex": "P(A \\cup B) = P(A) + P(B) - P(A \\cap B)",
          "meaning": "Probability of A or B occurring",
          "category": "rules"
        },
        {
          "id": "prob_and_indep",
          "name": "Multiplication Rule (AND, independent)",
          "formula": "P(A ‚à© B) = P(A) √ó P(B)",
          "latex": "P(A \\cap B) = P(A) \\times P(B)",
          "meaning": "For independent events",
          "category": "rules"
        },
        {
          "id": "prob_conditional",
          "name": "Conditional Probability",
          "formula": "P(A|B) = P(A ‚à© B) / P(B)",
          "latex": "P(A|B) = \\frac{P(A \\cap B)}{P(B)}",
          "meaning": "Probability of A given B occurred",
          "category": "rules"
        }
      ],
      "examples": [
        {
          "id": "prob_ex1",
          "question": "A die is rolled. Find P(even number) and P(number > 4)",
          "steps": [
            {
              "step": 1,
              "action": "Identify sample space",
              "result": "S = {1, 2, 3, 4, 5, 6}, total = 6",
              "explanation": "All possible outcomes"
            },
            {
              "step": 2,
              "action": "Find P(even)",
              "result": "Even = {2, 4, 6}, P(even) = 3/6 = 1/2",
              "explanation": "3 favorable out of 6"
            },
            {
              "step": 3,
              "action": "Find P(>4)",
              "result": "Greater than 4 = {5, 6}, P(>4) = 2/6 = 1/3",
              "explanation": "2 favorable out of 6"
            }
          ],
          "finalAnswer": "P(even) = 1/2, P(>4) = 1/3",
          "verification": "Each outcome equally likely",
          "difficulty": "easy"
        },
        {
          "id": "prob_ex2",
          "question": "Two coins are tossed. Find P(at least one head)",
          "steps": [
            {
              "step": 1,
              "action": "List sample space",
              "result": "S = {HH, HT, TH, TT}",
              "explanation": "All possible outcomes"
            },
            {
              "step": 2,
              "action": "Method 1: Count favorable",
              "result": "At least one H = {HH, HT, TH} = 3 outcomes\nP = 3/4",
              "explanation": "Direct counting"
            },
            {
              "step": 3,
              "action": "Method 2: Use complement",
              "result": "P(at least one H) = 1 - P(no H) = 1 - 1/4 = 3/4",
              "explanation": "Often easier for 'at least' problems"
            }
          ],
          "finalAnswer": "P(at least one head) = 3/4",
          "verification": "Both methods give same answer ‚úì",
          "difficulty": "easy"
        }
      ],
      "realWorldApplications": [
        {
          "title": "Weather Forecasting",
          "description": "Probability of rain, snow, etc."
        },
        {
          "title": "Insurance",
          "description": "Risk assessment and premium calculation"
        },
        {
          "title": "Games",
          "description": "Calculating odds in games of chance"
        }
      ]
