      "title": "Descriptive Statistics",
      "symbol": "xÃÑ, œÉ",
      "level": "middle",
      "definition": {
        "text": "Descriptive statistics summarize and describe the main features of a dataset. The three measures of central tendency (mean, median, mode) tell us about the 'typical' value, while measures of spread (range, variance, standard deviation) describe how spread out the data is.",
        "keyTerms": ["mean", "median", "mode", "range", "variance", "standard deviation"]
      },
      "keyFormulas": [
        {
          "id": "stat_mean",
          "name": "Mean (Average)",
          "formula": "xÃÑ = (Œ£x·µ¢)/n",
          "latex": "\\bar{x} = \\frac{\\sum x_i}{n}",
          "meaning": "Sum of all values divided by count",
          "category": "central_tendency"
        },
        {
          "id": "stat_median",
          "name": "Median",
          "formula": "Middle value when data is ordered",
          "latex": "\\text{Median} = \\text{middle value(s)}",
          "meaning": "50th percentile; if n is even, average of two middle values",
          "category": "central_tendency"
        },
        {
          "id": "stat_mode",
          "name": "Mode",
          "formula": "Most frequently occurring value",
          "latex": "\\text{Mode} = \\text{most frequent value}",
          "meaning": "Can have 0, 1, or multiple modes",
          "category": "central_tendency"
        },
        {
          "id": "stat_range",
          "name": "Range",
          "formula": "Range = Max - Min",
          "latex": "\\text{Range} = x_{max} - x_{min}",
          "meaning": "Difference between largest and smallest values",
          "category": "spread"
        },
        {
          "id": "stat_variance",
          "name": "Variance",
          "formula": "œÉ¬≤ = Œ£(x·µ¢ - xÃÑ)¬≤/n",
          "latex": "\\sigma^2 = \\frac{\\sum(x_i - \\bar{x})^2}{n}",
          "meaning": "Average squared deviation from mean",
          "category": "spread"
        },
        {
          "id": "stat_std",
          "name": "Standard Deviation",
          "formula": "œÉ = ‚àö[Œ£(x·µ¢ - xÃÑ)¬≤/n]",
          "latex": "\\sigma = \\sqrt{\\frac{\\sum(x_i - \\bar{x})^2}{n}}",
          "meaning": "Square root of variance; same units as data",
          "category": "spread"
        }
      ],
      "examples": [
        {
          "id": "stat_ex1",
          "question": "Find the mean, median, and mode of: 4, 7, 2, 9, 4, 5, 4, 8",
