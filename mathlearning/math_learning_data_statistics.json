    {
      "id": "counting_principles",
      "title": "Counting Principles",
      "symbol": "n!",
      "level": "high",
      "definition": {
        "text": "Counting principles help determine the number of ways events can occur. Permutations count arrangements where order matters; combinations count selections where order doesn't matter. The factorial n! = n √ó (n-1) √ó ... √ó 2 √ó 1.",
        "keyTerms": ["factorial", "permutation", "combination", "fundamental counting principle"]
      },
      "keyFormulas": [
        {
          "id": "count_factorial",
          "name": "Factorial",
          "formula": "n! = n √ó (n-1) √ó (n-2) √ó ... √ó 2 √ó 1",
          "latex": "n! = n \\times (n-1) \\times \\cdots \\times 2 \\times 1",
          "meaning": "Product of all positive integers up to n; 0! = 1",
          "category": "basic"
        },
        {
          "id": "count_permutation",
          "name": "Permutation",
          "formula": "P(n,r) = n!/(n-r)!",
          "latex": "P(n,r) = \\frac{n!}{(n-r)!}",
          "meaning": "Arrangements of r items from n (order matters)",
          "category": "arrangements"
        },
        {
          "id": "count_combination",
          "name": "Combination",
          "formula": "C(n,r) = n!/[r!(n-r)!]",
          "latex": "C(n,r) = \\binom{n}{r} = \\frac{n!}{r!(n-r)!}",
          "meaning": "Selections of r items from n (order doesn't matter)",
          "category": "selections"
        },
        {
          "id": "count_fcp",
          "name": "Fundamental Counting Principle",
          "formula": "If event A can occur in m ways and B in n ways, then A followed by B can occur in m √ó n ways",
          "latex": "m \\times n \\text{ total ways}",
          "meaning": "Multiply choices for sequential events",
          "category": "basic"
        }
      ],
      "examples": [
        {
          "id": "count_ex1",
          "question": "How many ways can 3 people be arranged in a line?",
          "steps": [
            {
              "step": 1,
              "action": "This is a permutation (order matters)",
              "result": "Arranging all 3 from 3 people",
              "explanation": "P(3,3) = 3!"
            },
            {
              "step": 2,
              "action": "Calculate 3!",
              "result": "3! = 3 √ó 2 √ó 1 = 6",
              "explanation": "All permutations"
            }
          ],
          "finalAnswer": "6 ways",
          "verification": "ABC, ACB, BAC, BCA, CAB, CBA - exactly 6",
          "difficulty": "easy"
        },
        {
          "id": "count_ex2",
          "question": "How many 5-card hands can be dealt from a standard 52-card deck?",
          "steps": [
            {
              "step": 1,
              "action": "This is a combination (order doesn't matter)",
              "result": "Selecting 5 from 52 cards",
              "explanation": "C(52,5)"
            },
            {
              "step": 2,
              "action": "Apply formula",
              "result": "C(52,5) = 52!/(5! √ó 47!)",
              "explanation": "n=52, r=5"
            },
            {
              "step": 3,
              "action": "Simplify",
              "result": "= (52√ó51√ó50√ó49√ó48)/(5√ó4√ó3√ó2√ó1) = 2,598,960",
              "explanation": "Cancel common factors"
            }
          ],
          "finalAnswer": "2,598,960 possible hands",
          "verification": "This is why poker is interesting!",
          "difficulty": "medium"
        },
        {
          "id": "count_ex3",
          "question": "A password has 4 digits (0-9). How many passwords are possible if repetition is allowed?",
          "steps": [
            {
              "step": 1,
              "action": "Apply Fundamental Counting Principle",
              "result": "10 choices for each digit",
              "explanation": "0-9 for each position"
            },
            {
              "step": 2,
              "action": "Multiply",
              "result": "10 √ó 10 √ó 10 √ó 10 = 10‚Å¥ = 10,000",
              "explanation": "Four independent choices"
            }
          ],
          "finalAnswer": "10,000 possible passwords",
          "verification": "From 0000 to 9999",
          "difficulty": "easy"
        }
      ],
      "realWorldApplications": [
        {
          "title": "Lottery",
